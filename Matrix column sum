#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
     int r,c,i,j;
    long long m[100][100],sum =0;
    cin>>r>>c;
    for(i=0;i<r;i++)
    {
        for(j=0;j<c;j++)
            cin>>m[i][j];
    }
    for(j=0;j<c;j++)
    {   sum = 0;
        for(i=0;i<r;i++)
        {sum += m[i][j];}
        
     cout<<sum<<endl;
    } 
}

#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
   int row,col,i,j;
    long long int m1[100][100],m2[100][100];
    cin>>row>>col;
    for(i=0;i<row;i++)
    {
        for (j=0;j<col;j++)
        {
            cin>>m1[i][j];
        }
    }
    for(i=0;i<row;i++)
    {
        for (j=0;j<col;j++)
        {
            cin>>m2[i][j];
        }
    }
    for(i=0;i<row;i++)
    {
        for (j=0;j<col;j++)
        {
            cout<<m1[i][j]+m2[i][j]<<" ";
        }
        cout<<endl;
    }
    
    
}

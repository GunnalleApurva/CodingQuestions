#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
   int n;
    cin>>n;
    int* a = new int[n];
    cin>>a[0];
    int max = a[0];
    for (int i=1;i<n;i++)
    {
        cin>>a[i];
        if ( max < a[i]) max = a[i];
    }
    cout<<max<<endl;
    delete [] a;
}
